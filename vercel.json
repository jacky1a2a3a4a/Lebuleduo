{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": "/Lebuleduo.svg", "dest": "/Lebuleduo.svg" },
    { "src": "/customer.*", "dest": "/index.html" },
    { "src": "/deliver.*", "dest": "/index.html" },
    { "src": "/auth.*", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
